        "System.Runtime.Serialization.Primitives": "4.1.1",
        "System.Threading": "4.0.11"
      }
    },
    "netstandard1.1": {
      "buildOptions": {
        "define": [ "NETSTANDARD", "NETSTANDARD1_1", "ENUM_MEMBER_ATTRIBUTE", "CONCURRENT_COLLECTIONS", "SECURITY_SAFE_CRITICAL", "COVARIANCE" ]
      },
      "dependencies": {
        "System.Collections": "4.0.11",
