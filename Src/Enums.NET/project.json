        "define": [ "NETSTANDARD", "NETSTANDARD1_3", "ENUM_MEMBER_ATTRIBUTE", "CONCURRENT_COLLECTIONS", "SECURITY_SAFE_CRITICAL", "COVARIANCE", "ICONVERTIBLE" ]
      },
      "dependencies": {
        "System.Collections": "4.0.11",
        "System.Collections.Concurrent": "4.0.12",
